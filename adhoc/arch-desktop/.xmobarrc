Config { font = "xft:Sans-9:bold"
       , bgColor = "black"
       , fgColor = "grey"
       , alpha   = 240
       , position = Top
       , lowerOnStart = False
       , commands = [ Run Weather "EGPF" ["-t"," <tempF>F","-L","64","-H","77","--normal","green","--high","red","--low","lightblue"] 36000
                    , Run Network "enp0s25" ["-L","0","-H","32","--normal","green","--high","red"] 10
                    , Run MultiCpu ["-t","<total0>,<total1>,<total2>,<total3>%","-L","3","-H","50","--normal","green","--high","red"] 10
                    , Run Memory ["-t","Mem: <usedratio>%"] 10
                    , Run Swap [] 10
                    , Run Date "%a %b %_d %Y %H:%M:%S" "date" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ %multicpu% | %memory% * %swap% | %enp0s25%   <fc=#ee9a00>%date%</fc> | %EGPF%"
       }
