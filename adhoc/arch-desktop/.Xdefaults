URxvt*termName: xterm

!URxvt.font: xft:IPAMonaGothic:size=12
URxvt.font: xft:Ricty:size=12
URxvt.boldFont: xft:Ricty-Bold:size=12
URxvt.boldItalicFont: xft:IPAMonaGothic:size=12
URxvt.italicFont: xft:IPAMonaGothic:size=12

!!! colors
URxvt.foreground: #ffffff
URxvt*shading: 20
URxvt*background: rgba:1111/1111/1111/dddd
URxvt*cursorColor: #9a9a9a

! Black (not tango) + DarkGrey
*color0:  #000000
*color8:  #555753
! DarkRed + Red
*color1:  #ff6565
*color9:  #ff8d8d
! DarkGreen + Green
*color2:  #93d44f
*color10: #c8e7a8
! DarkYellow + Yellow
*color3:  #eab93d
*color11: #ffc123
! DarkBlue + Blue
*color4:  #204aa7
*color12: #3465c4
! DarkMangenta + Mangenta
*color5:  #ce5c00
*color13: #f57900
!DarkCyan + Cyan (both not tango)
*color6:  #89b6e2
*color14: #46a4ff
! LightGrey + White
*color7:  #cccccc
*color15: #ffffff


!URxvt.imFont: xft:fixed
URxvt*inheritPixmap: True
URxvt*preeditType: OverTheSpot
URxvt*inputMethod: scim
Rxvt.geometry: 150x55+5+5
Rxvt.scrollBar: false
!Rxvt.borderLess: true

! Emacs

! medium-r-normal
Emacs.Fontset-0: xft:Ricty:size=12
