#compdef pythonbrew

_arguments -C -s \
    'install[a]' \
    'switch[a]' \
    'use[a]' \
    'py[a]' \
    'list -k[a]' \
    'uninstall[a]' \
    'cleanup[a]' \
    'update[a]' \
    'off[a]' \
    'symlink[a]' \
    'buildout[a]' \
    'venv[a]' \
    'version[a]' \
    'See more details below[a]' \
  '*:file:_files' && return 0
return 1
